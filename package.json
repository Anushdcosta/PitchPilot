{
  "name": "startup-pitcher",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:imagegen\" \"npm run dev:backend\" \"npm run dev:frontend\" ",
    "dev:backend": "tsx src/index.ts",
    "dev:frontend": "npm --prefix frontend run dev",
    "dev:imagegen": "python imagegen.py",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "ts-node": {
    "esm": true
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Startup pitch generator with AI-powered image generation",
  "dependencies": {
    "@huggingface/inference": "^2.6.4",
    "@langchain/community": "^0.3.45",
    "@langchain/ollama": "^0.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "html-to-image": "^1.11.13",
    "html2pdf.js": "^0.10.3",
    "jspdf": "^3.0.1",
    "langchain": "^0.3.27",
    "lucide-react": "^0.511.0",
    "node-fetch": "^3.3.2",
    "react-router-dom": "^7.6.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/node": "^22.15.23",
    "concurrently": "^9.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
